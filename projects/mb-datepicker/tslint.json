{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "mb",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "mb",
      "kebab-case"
    ],
      "no-host-metadata-property": false,
      "one-variable-per-declaration": false
  }
}
